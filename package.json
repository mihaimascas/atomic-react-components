{
  "name": "atomic-react-components",
  "version": "0.3.0",
  "description": "Atomoc design applied to react components",
  "scripts": {
    "test": "mocha --require ignore-styles --require jsdomSetup.js --compilers js:babel-register ./src/**/*.spec.js --recursive",
    "test:watch": "npm test -- --watch",
    "clean": "rimraf dist",
    "build:lib": "webpack --config webpack.config.lib.js",
    "build:site": "webpack --config webpack.config.site.js",
    "build:webpack:theme:gray": "NODE_ENV=production webpack --config webpack.config.prod.js --theme gray",
    "build:webpack:theme:red": "NODE_ENV=production webpack --config webpack.config.prod.js --theme red",
    "build:webpack:theme:blue": "NODE_ENV=production webpack --config webpack.config.prod.js --theme blue",
    "build": "npm run clean && npm run build:webpack:theme:gray && npm run build:webpack:theme:red && npm run build:webpack:theme:blue",
    "start": "node devServer.js",
    "lint": "eslint src",
    "doc": "esdoc -c .esdocrc"
  },
  "repository": {
    "type": "git",
    "url": "https://git.accesa.eu/mihai.mascas/atomic-components-poc"
  },
  "keywords": [
    "react",
    "reactjs",
    "library",
    "UI",
    "components",
    "webpack",
    "babel",
    "BEM",
    "scss",
    "TDD",
    "enzyme",
    "mocha",
    "sinom"
  ],
  "author": "Mihai Mascas",
  "homepage": "https://git.accesa.eu/mihai.mascas/atomic-components-poc",
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "autoprefixer-core": "^6.0.1",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "css-loader": "^0.26.0",
    "enzyme": "^2.5.1",
    "esdoc": "^0.4.8",
    "esdoc-es7-plugin": "0.0.3",
    "eslint-config-airbnb": "^1.0.2",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "except": "^0.1.3",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "ignore-loader": "^0.1.2",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.2",
    "mocha-jsdom": "^1.1.0",
    "node-sass": "^3.4.2",
    "postcss-loader": "^1.1.1",
    "prismjs": "^1.6.0",
    "react-addons-test-utils": "^0.14.8",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.0.1",
    "rimraf": "^2.4.3",
    "sass-loader": "^4.0.2",
    "sass-mq": "^3.2.9",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "react-router": "^3.0.0",
    "redux": "^3.0.4",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  }
}
